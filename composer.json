{
    "name": "onyxerp/silexbase",
    "description": "Silex Base para criar API REST para OnyxERP",
    "version": "1.0.4",
    "type": "library",
    "time": "2017-03-27",
    "homepage": "https://github.com/BRAConsultoria/SilexBase",
    "license": "proprietary",
    "authors": [{
            "name": "Community contributors",
            "homepage": "https://github.com/BRAConsultoria/SilexBase/graphs/contributors"
        }
    ],
    "support": {
        "email": "suporte@grupobra.com",
        "issues": "https://github.com/BRAConsultoria/SilexBase/issues",
        "wiki": "https://github.com/BRAConsultoria/SilexBase/wiki"
    },
    "require": {
        "php": ">=5.6",
        "silex/silex": "~2.0",
        "symfony/config": "*",
        "symfony/validator": "*",
        "guzzlehttp/guzzle": "*",
        "monolog/monolog": "*"
    },
    "autoload": {
        "psr-4": {
            "OnyxERP\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/",
            "/test/",
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OnyxERP\\": "src/"
        }
    },
    "require-dev": {
        "behat/behat": "*",
        "behat/mink-extension": "*",
        "behat/mink-goutte-driver": "*",
        "behat/mink-selenium2-driver": "*",
        "phpspec/phpspec": "*",
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "*",
        "pdepend/pdepend": "*",
        "phpunit/phpunit-mock-objects": "*",
        "phpmetrics/phpmetrics": "*",
        "phpmd/phpmd": "*",
        "phploc/phploc": "*",
        "sebastian/phpcpd": "*",
        "friendsofphp/php-cs-fixer": "*",
        "braconsultoria/jwt-api-extension": "*"
    },
    "repositories": [{
            "type": "git",
            "url": "https://github.com/BRAConsultoria/JWTApiExtension"
        }
    ],
    "config": {
        "bin-dir": "bin/"
    }
}
